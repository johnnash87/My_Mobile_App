<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ie.a10358323mydbs.my_mobile_app">

    <uses-permission android:name="android.permission.INTERNET">
        <uses-permission android:name="android.permission.CAMERA" />
    </uses-permission>
        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dublin_bikes"
        android:label="Dublin Bikes"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".LoginActivity"
            android:label="Dublin Bikes"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".NetworkingAndroidHttpClientJSONActivity"
            android:label="string/app_name" />
        <activity
            android:name=".MainActivity"
            android:label="string/app_name" />
        <activity
            android:name=".CameraUpload"
            android:label="@string/title_activity_camera_upload"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Journey"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".MapsActivity"
            android:label="MapsActivity"
            android:theme="@style/AppTheme.NoActionBar">


        </activity>

    </application>

</manifest>